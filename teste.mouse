void Login(TUsuario adm)
{
    system("cls");


    TUsuario usuario;

    Janela();
    //Cor_do_BG(15);
    system("color 3F");

    //textcolor(0);
    //Janela do usuario e senha
    {
        int i;

    gotoxy(9,11);
    printf("%c",186);

    gotoxy(9,12);
    printf("%c",204);
    gotoxy(9,10);
    printf("%c",201);
    for(i=10;i<35;i++)
    {
        gotoxy(i,10);
        printf("%c",205);
    }
    for(i=10;i<35;i++)
    {
        gotoxy(i,12);
        printf("%c",205);
    }

    gotoxy(35,12);
    printf("%c",185);

    gotoxy(35,10);
    printf("%c",187);

    gotoxy(35,11);
    printf("%c",186);

    //Janela senha
    for(i=10;i<35;i++)
    {
        gotoxy(i,14);
        printf("%c",205);
    }

    gotoxy(35,14);
    printf("%c",188);

    gotoxy(35,13);
    printf("%c",186);

    gotoxy(9,13);
    printf("%c",186);

    gotoxy(9,14);
    printf("%c",200);

    }



    gotoxy(10,11);
    printf("Usuario:");
    gotoxy(10,13);
    printf("Senha:");

    Cadeado_Fechado();

    int cont=0,i,j;

    do
    {
        cont=0;

        for(i=0;i<10;i++)
        {
           usuario.senha[i]=' ';
        }

        gotoxy(19,11);
        gets(usuario.usuario);
        j=17;
        for(i=0;i<10;i++)
        {

            gotoxy(j,13);
            usuario.senha[i]=getch();

            if(usuario.senha[i]==13)
            {
                break;
            }
            else if(usuario.senha[i]==8)
            {
                gotoxy(j-1,13);
                printf(" ");
                j--;
                i--;
                cont--;
            }

            else
            {
                cont++;
                j++;
               printf("*");
            }

        }
        j=0;
        for(i=0;i<cont;i++)
        {
            if(adm.senha[i]!=usuario.senha[i])
            {
                j++;
            }

        }
        if(strcmp(usuario.usuario,adm.usuario)!=0 || j!=0)
        {
            gotoxy(10,15);
            printf("Usuario ou senha incorretos.");
            gotoxy(10,16);
            printf("Tente novamente\n\n\n\n\n\n");

            Sleep(1000);
            //Para apagar as linhas
            gotoxy(19,11);
            printf("              ");
            gotoxy(17,13);
            printf("               ");
            gotoxy(10,15);
            printf("                            ");
            gotoxy(10,16);
            printf("               ");
        }
    }while(strcmp(usuario.usuario,adm.usuario)!=0 || j!=0);
    Cadeado_abrindo();
}
